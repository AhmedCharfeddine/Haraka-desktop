<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<SolidColorBrush x:Key="PrimaryBrush" Color="#912d53" />
	<SolidColorBrush x:Key="SecondaryBrush" Color="#E5E7EB" />
	<SolidColorBrush x:Key="TextBrush" Color="#F9FAFB" />
	<SolidColorBrush x:Key="SubtleTextBrush" Color="#999999"/>
	<SolidColorBrush x:Key="CodeBackgroundBrush" Color="#1e1e2e"/>
	<!--<SolidColorBrush x:Key="WindowBackgroundBrush" Color="#230a13" />-->
	<LinearGradientBrush x:Key="WindowBackgroundBrush" StartPoint="0,0" EndPoint="0,1">
		<GradientStop Color="#1e1e2f" Offset="0" />
		<GradientStop Color="#230a13" Offset="1" />
	</LinearGradientBrush>

	<SolidColorBrush x:Key="TextAreaBackgroundBrush" Color="#15060c"/>
	<SolidColorBrush x:Key="BorderBrush" Color="#474747"/>
</ResourceDictionary>
